{
	"miniprogramRoot": "miniprogram/",
	"setting": {
		"urlCheck": false,
		"es6": true,
		"postcss": true,
		"minified": true,
		"newFeature": true,
		"nodeModules": true
	},
	"appid": "<%= appid %>",
	"projectname": "<%= appName %>",
	"libVersion": "2.0.9",
	"packOptions": {
		"ignore": [
			{
				"type": "folder",
				"value": "node_modules"
			},
			{
				"type": "suffix",
				"value": ".map"
			},
			{
				"type": "suffix",
				"value": ".css"
			}
		]
	},
	"scripts": {
		"beforeCompile": "echo 1",
		"beforePreview": "echo 2",
		"beforeUpload": "./bin/beforeUpload.sh"
	},
	"compileType": "miniprogram"
}
